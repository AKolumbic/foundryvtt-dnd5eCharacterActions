{
  "name": "actions-tab-5e",
  "version": "8.0.0",
  "description": "A module for FoundryVTT's D&D 5e system that adds an Actions tab to the character sheet",
  "scripts": {
    "test": "jest",
    "build": "gulp build",
    "dev": "gulp watch",
    "lint": "eslint ."
  },
  "author": "drosshole",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drosshole/actions-tab-vtt-module.git"
  },
  "bugs": {
    "url": "https://github.com/drosshole/actions-tab-vtt-module/issues"
  },
  "homepage": "https://github.com/drosshole/actions-tab-vtt-module#readme",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.3.1",
    "eslint": "^8.27.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^30.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "setupFiles": [
      "./tests/setup.js"
    ]
  }
}
